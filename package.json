{
	"name": "root",
	"version": "0.0.0",
	"description": "Redux reduced",
	"scripts": {
		"prepublishOnly": "npm run build",
		"deploy": "lerna publish --yes --conventional-commits --github-release",
		"acp": "git add . && npm run commit && git push",
		"commit": "git-cz",
		"lint": "eslint **/*.js --fix",
		"test": "jest ",
		"build": "rollup -c",
		"bootstrap": "lerna bootstrap --hoist",
		"doc": "node_modules/.bin/jsdoc --configure .jsdoc.json --verbose",
		"test:cov": "jest --coverage",
		"test:watch": "jest --watchAll"
	},
	"devDependencies": {
		"@babel/cli": "7.4.3",
		"@babel/core": "7.4.3",
		"@babel/preset-env": "7.4.3",
		"commitizen": "3.0.7",
		"cz-conventional-changelog": "2.1.0",
		"eslint": "5.16.0",
		"eslint-config-standard": "12.0.0",
		"eslint-plugin-array-func": "3.1.3",
		"eslint-plugin-import": "2.17.1",
		"eslint-plugin-jest": "22.4.1",
		"eslint-plugin-no-loops": "0.3.0",
		"eslint-plugin-node": "8.0.1",
		"eslint-plugin-promise": "4.1.1",
		"eslint-plugin-simple-import-sort": "3.1.0",
		"eslint-plugin-sonarjs": "0.3.0",
		"eslint-plugin-standard": "4.0.0",
		"eslint-plugin-unicorn": "8.0.2",
		"expect-more-jest": "2.4.1",
		"jest": "24.7.1",
		"jest-chain": "1.1.2",
		"jest-extended": "0.11.1",
		"jsdoc": "3.5.5",
		"lerna": "3.13.2",
		"minami": "1.2.3",
		"rollup": "1.10.0",
		"rollup-plugin-babel": "4.3.2",
		"rollup-plugin-babel-minify": "8.0.0",
		"rollup-plugin-node-resolve": "5.2.0"
	},
	"dependencies": {
		"@babel/polyfill": "7.4.3"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/elcoosp/rxdxx.git"
	},
	"homepage": "https://github.com/elcoosp/rxdxx",
	"bugs": "https://github.com/elcoosp/rxdxx/issues",
	"license": "MIT",
	"author": {
		"name": "Louis Cospain",
		"url": "https://lcospain.com/"
	},
	"keywords": [
		"redux",
		"reducer",
		"action creators",
		"state",
		"state management",
		"action types",
		"relations"
	]
}
